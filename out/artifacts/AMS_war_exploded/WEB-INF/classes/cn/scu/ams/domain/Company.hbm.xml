<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.scu.ams.domain.Company" table="company">
        <id name="companyId" column="companyId">
            <generator class="native"></generator>
        </id>
        <property name="customerId" column="customerId" length="20"></property>
        <property name="companyName" column="companyName" length="20"></property>
        <property name="applyCustomer" column="applyCustomer" length="20"></property>
        <property name="customerIdNumber" column="customerIdNumber" length="20"></property>
        <property name="customerPhone" column="customerPhone" length="20"></property>
        <property name="customerEmail" column="customerEmail" length="20"></property>
        <property name="customerAddress" column="customerAddress" length="45"></property>
        <property name="companyAddress" column="companyAddress" length="45"></property>
        <property name="managerArea" column="managerArea" length="20"></property>
        <property name="managerProducts" column="managerProducts" length="20"></property>
        <property name="companyDescribe" column="companyDescribe" length="200"></property>
        <property name="registerMoney" column="registerMoney" length="20"></property>
        <property name="uploadFilesPath" column="uploadFilesPath" length="50"></property>
        <property name="existStatus" column="existStatus" length="20"></property>

    </class>
</hibernate-mapping>
