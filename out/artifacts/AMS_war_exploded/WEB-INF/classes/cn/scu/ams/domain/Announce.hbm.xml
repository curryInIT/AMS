<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.scu.ams.domain.Announce" table="announce">
        <id name="announceId" column="announceId">
            <generator class="native"></generator>
        </id>
        <property name="contentTitle" column="contentTitle" length="50"></property>
        <property name="content" column="content" length="200"></property>
        <property name="employeeId" column="employeeId" length="11"></property>
        <property name="pubTime" column="pubTime" length="20"></property>
        <property name="existStatus" column="existStatus" length="11"></property>

    </class>
</hibernate-mapping>